	keywords = {system of systems engineering, combat modeling, federated simulation system, West Point department of systems engineering},
	pages = {765--810},
	file = {Snapshot:/Users/hcadavid/Zotero/storage/WYQ2FJFK/9781118180310.html:text/html}
