	author = {Daneva, Maya},
	keywords = {COCOMO, Enterprise Resource Planning implementation, Monte Carlo simulation, portfolio management, project effort estimation},
	pages = {329--357},
	file = {Full Text PDF:/Users/hcadavid/Zotero/storage/XBNK2YLA/Daneva - Balancing uncertainty of context in ERP project es.pdf:application/pdf;Snapshot:/Users/hcadavid/Zotero/storage/UFGQNST6/smr.html:text/html}
