	pages = {14--23}
}

@inproceedings{vargas_approaches_2016-1,
	title = {Approaches for {Integration} in {System} of {Systems}: {A} {Systematic} {Review}},
	shorttitle = {Approaches for {Integration} in {System} of {Systems}},
	doi = {10.1109/SESoS.2016.014},
	abstract = {Software systems have become increasingly complex, and they are often formed by integrating independent systems, resulting in a new class of systems referenced as System of Systems (SoS). The System of Systems Integration (SoSI) emerges as a new challenge and aims to create a new feature by integrating constituent systems that contribute to the overall goal of the SoS. Often the constituent systems tend to be from different sources and behaviors and, as such, tend to employ different terminology and concepts. Also, when SoSI involves the integration of legacy systems, cases where the documentation and the necessary skills for the harmonious integration are readily available are very rare. However, it is observed that there is a lack of studies that are comprehensive and, at the same time, contain a detailed view of how the constituent systems are integrated in order to collectively achieve a common goal. Based on this scenario, the main contribution of this systematic review (SR) is to investigate the state of the art of SoSI and the software engineering methods that assist in the integration between constituent systems of a SoS. The SR has found 1398 studies and, at the end of the selection process, we selected 29 studies for data extraction. Most studies describe individuals and teams who have worked in isolation to develop solutions to certain problems without widespread adoption of a form of integration. Thus, there is a growing concern of researchers in the SoS context. However, it still lacks research and greater dissemination of concepts among researchers in the field.},
	booktitle = {2016 {IEEE}/{ACM} 4th {International} {Workshop} on {Software} {Engineering} for {Systems}-of-{Systems} ({SESoS})},
	author = {Vargas, I. G. and Gottardi, T. and Braga, R. T. V.},
	month = may,
	year = {2016},
	keywords = {Complexity theory, Context, System of systems, Data mining, Databases, Systematics, Interoperability, Software engineering, Integration, System of Systems Integration, Systematic Review},
	pages = {32--38},
	file = {IEEE Xplore Abstract Record:/Users/hcadavid/Zotero/storage/K4N396MK/7809805.html:text/html}
}

@inproceedings{li_method_2013,
	title = {A method of {WESoS} capability assessment based on {DM}2 and {ABS}},
	volume = {3},
	doi = {10.1109/ICIII.2013.6703521},
	abstract = {Modeling and Simulation is important at all stages of weapon equipment system-of-systems (WESoS) development. However, little work has been put into taking data from the architectures and exercising them in a constructive combat simulation environment, and cannot take the advantage of data-centric approach adopted by DoDAF2.0 either. This paper details a method for finding architecturally feasible alternatives to WESoS capability problems in early phase acquisition using data meta-model (DM2) and agent-based simulation (ABS). Firstly, a literature review of SoS architecture description and executable architecture modeling and analysis was studied, then capabilities-based DM2 (CBDM2) was developed from the view of capability assessment. Next, based on the CBDM2, a method of mapping CBDM2 to ABS system was proposed. The method proposed is a preliminary study on meta-model based architecture development and executable model in constructive combat simulation to WESoS capability assessment.},
